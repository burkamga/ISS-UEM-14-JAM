    private void btSalvarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btSalvarActionPerformed
        //Confere senhas iguais
        String senha1 = new String(pfSenha.getPassword());
        String senha2 = new String(pfRepitaSenha.getPassword());
        if(senha1.equals(senha2)){
            //Chama função de adição na control
            int id = Integer.parseInt(cmpID.getText());
            boolean resultado = COMedico.attMedico(id,cmpNome.getText(), cmpRG.getText(), cmpCPF.getText(), 
            dcDataNasc.getDate(), cmpEndereco.getText(), cmpTelefone.getText(), 
            cmpCelular.getText(), cmpEmail.getText(), cmpNCrm.getText(), 
            cmpEspecializacao.getText(), cmpLogin.getText(), senha1);
            //Se OK, volta pra tela ADM
            if(resultado){
                TelaAdm voltaTela = new TelaAdm();
                voltaTela.setLocationRelativeTo(null);
                voltaTela.setVisible(true);
                this.dispose();
            }
        }else{
            JOptionPane.showMessageDialog(rootPane, "As senhas não conferem!");
        }
    }//GEN-LAST:event_btSalvarActionPerformed

    private void btExcluirMedicoActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btExcluirMedicoActionPerformed
        int id = Integer.parseInt(cmpID.getText());
        int result = JOptionPane.showConfirmDialog(rootPane, "Tem Certeza que deseja remover este médico?");
        if(result == 0){
            boolean resultado = COMedico.excluiMedico(id);
            if(resultado){
                TelaAdm voltaTela = new TelaAdm();
                voltaTela.setLocationRelativeTo(null);
                voltaTela.setVisible(true);
                this.dispose();
            }
        }
        
        
        
    }//GEN-LAST:event_btExcluirMedicoActionPerformed

