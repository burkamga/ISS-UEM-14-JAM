    public TelaAttMedico(Vomedico medico) {
        initComponents();
        //Atualiza os campos com o Objeto Médico passado por parametro
        cmpNome.setText(medico.getNome());
        cmpRG.setText(medico.getRg());
        cmpCPF.setText(medico.getCpf());
        dcDataNasc.setDate(medico.getDataNascimento());
        cmpTelefone.setText(medico.getTelefone());
        cmpCelular.setText(medico.getCelular());
        cmpNCrm.setText(medico.getNumCrm());
        cmpEndereco.setText(medico.getEndereco());
        cmpEmail.setText(medico.getEmail());
        cmpEspecializacao.setText(medico.getEspecializacao());
        cmpLogin.setText(medico.getUsename());
        String id = String.valueOf(medico.getIdPessoa());
        cmpID.setText(id);
    }

